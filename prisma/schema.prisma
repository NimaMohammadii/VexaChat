generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id          String   @id @default(uuid())
  name        String
  age         Int
  city        String
  price       Int
  description String
  images      String[]
  createdAt   DateTime @default(now())
}
